{
  "keymaps": {
    "0": { "type": "scroll.home" },
    ":": { "type": "command.show" },
    "k": { "type": "scroll.vertically", "count": -2 },
    "j": { "type": "scroll.vertically", "count": 2 },
    "h": { "type": "scroll.horizonally", "count": -1 },
    "l": { "type": "scroll.horizonally", "count": 1 },
    "gg": { "type": "scroll.top" },
    "G": { "type": "scroll.bottom" },
    "$": { "type": "scroll.end" },
    "D": { "type": "tabs.close", "select": "left" },
    "x$": { "type": "tabs.close.right" },
    "!d": { "type": "tabs.close", "force": true },
    "K": { "type": "tabs.prev" },
    "J": { "type": "tabs.next" },
    "gT": { "type": "tabs.prev" },
    "gt": { "type": "tabs.next" },
    "g0": { "type": "tabs.first" },
    "g$": { "type": "tabs.last" },
    "<C-6>": { "type": "tabs.prevsel" },
    "r": { "type": "tabs.reload", "cache": false },
    "R": { "type": "tabs.reload", "cache": true },
    "zp": { "type": "tabs.pin.toggle" },
    "zd": { "type": "tabs.duplicate" },
    "zi": { "type": "zoom.in" },
    "zo": { "type": "zoom.out" },
    "zz": { "type": "zoom.neutral" },
    "f": { "type": "hint.quick", "newTab": false },
    "F": { "type": "hint.quick", "newTab": true, "background": false },
    ";y": { "type": "hint.yankurl" },
    ";Y": { "type": "hint.yanklinktext" },
    ";i": { "type": "hint.openimage", "newTab": false },
    ";I": { "type": "hint.openimage", "newTab": true, "background": false },
    ";v": { "type": "hint.source", "newTab": false },
    ";V": { "type": "hint.source", "newTab": true },
    ";o": { "type": "hint.open" },
    ";t": { "type": "hint.tabopen" },
    ";w": { "type": "hint.winopen" },
    ";O": { "type": "hint.command.open" },
    ";T": { "type": "hint.command.tabopen" },
    ";W": { "type": "hint.command.winopen" },
    "m": { "type": "mark.set.prefix" },
    "'": { "type": "mark.jump.prefix" },
    "H": { "type": "navigate.history.prev" },
    "L": { "type": "navigate.history.next" },
    "[[": { "type": "navigate.link.prev" },
    "]]": { "type": "navigate.link.next" },
    "gu": { "type": "navigate.parent" },
    "gU": { "type": "navigate.root" },
    "gi": { "type": "focus.input" },
    "gf": { "type": "navigate.source" },
    "gh": { "type": "navigate.home" },
    "gH": { "type": "navigate.home", "newTab": true },
    "gr": { "type": "tabs.reader.toggle" },
    "y": { "type": "urls.yank" },
    "p": { "type": "urls.paste", "newTab": false },
    "P": { "type": "urls.paste", "newTab": true },
    "/": { "type": "find.start" },
    "n": { "type": "find.next" },
    "N": { "type": "find.prev" },
    ".": { "type": "repeat" },
    "<S-Esc>": { "type": "addon.toggle.enabled" }
  },
  "search": {
    "default": "google",
    "engines": {
      "google": "https://google.com/search?q={}"
    }
  },
  "properties": {
    "hintchars": "asdqwerzxcv",
    "smoothscroll": true,
    "complete": "sbh",
    "colorscheme": "system",
    "ignorecase": true,
    "findmode": "normal"
  },
  "blacklist": [
  ],
  "styles": {
    "hint": {
      "min-width": "25px",
      "text-align": "center",
      "border-radius": "6px",
      "background-color": "#b4befe",
      "padding": "2px",
      "font-weight": "bold",
      "font-size": "14px",
      "color": "#1e1e2e"
    },
    "console": {
      "font-family": "IosevkaTerm Nerd Font",
      "font-size": "12px",
      "background-color": "#1e1e2e",
      "color": "#b4befe"
    }
  }
}
